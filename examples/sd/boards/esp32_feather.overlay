
&uart0 {
    current-speed = <921600>;
};

&spi2 {
    clock-frequency = <8000000>;
    cs-gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
    arducam_mega0: arducam-mega0@0 {
        compatible = "arducam,mega";
        reg = <0>;
        spi-max-frequency = <8000000>;
        status = "okay";
    };
};